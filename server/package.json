{
  "name": "cooler-server",
  "version": "1.0.0",
  "description": "Cooler server.",
  "private": true,
  "main": "build/index",
  "dependencies": {
    "a18n": "^1.5.1",
    "apollo-server-express": "^2.17.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
